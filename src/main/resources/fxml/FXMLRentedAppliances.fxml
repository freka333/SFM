<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="554.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appliances.view.FXMLRentedAppliancesController">
   <children>
      <Label layoutX="14.0" layoutY="23.0" text="Bérelt eszközeim">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="applistButton" layoutX="209.0" layoutY="19.0" mnemonicParsing="false" onAction="#applistButtonPushed" text="Eszközök listája">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="newApplianceButton" layoutX="501.0" layoutY="19.0" mnemonicParsing="false" onAction="#newApplianceButtonPushed" text="Saját eszköz megosztása">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Pane layoutX="12.0" layoutY="63.0" prefHeight="442.0" prefWidth="675.0">
         <children>
            <ScrollPane prefHeight="442.0" prefWidth="675.0">
               <content>
                  <VBox fx:id="itemsLayout" prefHeight="440.0" prefWidth="660.0" />
               </content>
            </ScrollPane>
         </children>
      </Pane>
      <Button fx:id="logoutButton" layoutX="602.0" layoutY="515.0" mnemonicParsing="false" onAction="#logoutButtonPushed" text="Kijelentkezés" />
      <Button fx:id="sharedApplianceButton" layoutX="333.0" layoutY="19.0" mnemonicParsing="false" onAction="#sharedApplianceButtonPushed" text="Megosztott eszközeim">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
